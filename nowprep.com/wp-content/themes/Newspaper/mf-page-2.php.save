<script>
    $(document).ready(function() {
        $(".progress").asProgress({
            namespace: 'progress',
            speed: 100, // refresh speed
            delay: 100
        });
        $('.progress').asProgress('start');
        var loading_content = $('.loading_content').find('p');
        var loading_content_length = loading_content.length - 1;
        var i = 0;
        var timer = setTimeout(function run() {
            if(i <= loading_content_length) {
                var prev = i - 1;
                var next = i + 1;
                if(i >= 0 && i < 2) {
                    if ( i > 0) {
                        loading_content.eq(prev).removeClass('loading').addClass('completed');
                    }
                    loading_content.eq(i).addClass('loading');
                    var timer1 = setTimeout(run, 5000);
                }
                if( i === 2) {
                    loading_content.eq(prev).removeClass('loading').addClass('completed');
                    setTimeout(function() {
                        loading_content.eq(2).addClass('completed_without_checkbox').show();
                        loading_content.eq(3).addClass('loading').show();
                        $('.progress').asProgress('reset');
                        $(".progress").asProgress("start");
                        var timer1 = setTimeout(run, 10000);
                    }, 1000);
                }
                if ( i === 3) {
                    loading_content.eq(i).removeClass('loading').addClass('completed');
                }
                i += 1;
            }
        }, 0);
    });
</script>
